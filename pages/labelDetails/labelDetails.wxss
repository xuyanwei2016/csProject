/* pages/labelDetails/labelDetails.wxss */
.labelDet_tab{
  width:100%;
  height:80rpx;
  border-bottom:1rpx solid #E3E3E3;
  position:relative;
}
.tab_text{
  width:25%;
  height:47rpx;
  float:left;
  text-align: center;
  font-size:28rpx;
  box-sizing:border-box;
  margin-top:18rpx;
  line-height:47rpx;
  color:#666;
}
.table_text_blue{
  width:25%;
  height:47rpx;
  float:left;
  text-align: center;
  font-size:28rpx;
  box-sizing:border-box;
  margin-top:18rpx;
  line-height:47rpx;
  color:#2957BB;
}
.table_blue{
  color:#2957BB;
}

.tab_new{
  border-left:1rpx solid #E3E3E3;
}  
.tab_img{
  width:14rpx;
  height:8rpx;
  margin-left:9rpx;
  margin-bottom:7rpx;
}

.tab_come{
  width:100%;
  height:234rpx;
  background:#fff;
  position:absolute;
  top:82rpx;
  left:0;
}
.tab_come_top{
  margin:40rpx 0 30rpx 0;
}
.tab_come_text{
  width:158rpx;
  height:62rpx;
  float:left;
  background:rgba(245,247,250,1);
  border-radius:31px;
  text-align:center;
  line-height:62rpx;
  font-size:28rpx;
  margin-left:24rpx;
  margin-top:36rpx;
  color:#666;
}
.tab_click{
  width:158rpx;
  height:62rpx;
  float:left;
  border-radius:31px;
  text-align:center;
  line-height:62rpx;
  font-size:28rpx;
  margin-left:24rpx;
  margin-top:36rpx;
  background:#3477EB;
  color:#fff;
}
.label_mask{
  width:100%;
  height:100%;
  background:rgba(0,0,0,1);
  opacity:0.5;
  position:fixed;
  top:316rpx;
  left:0;
}
.label_over{
  margin: 35rpx auto 44rpx;
  color: #D5D7E0;
  font-size: 12px;
  font-weight:500;
  line-height: 40rpx;
  text-align: center;
}
