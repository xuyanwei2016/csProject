#book_page{
  border-top: solid 1px #EEEEEE;
}
.collect_list{
  margin: 0 24rpx;
  border-bottom: solid 1px #EEEEEE;
  overflow: hidden;
  display: flex;
  justify-content: space-between;
}
.content {
  width: 100%;
  margin-right: 0;
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
  -webkit-transform: translateX(150rpx);
  transform: translateX(150rpx);
  margin-left: -150rpx;
  display: flex;
}
.collect_left{
  height: 200rpx;
  width: 300rpx;
  margin:28rpx 0;
  margin-right: 24rpx;
}
.collect_cover{
  height: 200rpx;
  width: 300rpx;
}
.collect_text{
  flex: 1;
  margin-top: 28rpx;
  font-weight:bold;
  font-size: 15px;
  color: #202021;
  line-height:44rpx;
  white-space: normal;
  overflow: hidden;
  word-break: break-all;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2;
  text-overflow:ellipsis;
  display: -webkit-box;
  height: 88rpx;
  margin-right: 24rpx;
}
/*无数据*/
.resource_null{
  text-align: center;
}
.resource_null .null_image{
  width: 435rpx;
  height: 409rpx;
  margin: 0 auto;
  font-weight:500;
  margin-top: 180rpx;
}
.resource_null .null_text{
  margin-top: 50rpx;
  color: #696969;
  font-size: 16px;
}
.resource_null .null_tips{
  margin-top: 19rpx;
  color: #BDC2CB;
  font-size: 14px;
}
/*滑动删除*/
.del {
  background-color: #F2332F;
  width: 150rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  color: #fff;
  -webkit-transform: translateX(150rpx);
  transform: translateX(150rpx);
  -webkit-transition: all 0.4s;
  transition: all 0.4s;
}
 
.touch-move-active .content, .touch-move-active .del {
  -webkit-transform: translateX(0);
  transform: translateX(0);
}