/* pages/labelReview/labelReview.wxss */
.label_search{
  width: 100%;
  background: url('http://www.zgsccbs.com/skz/program/label_bg.png');
  background-size: 100%;
  background-repeat: no-repeat;
  overflow: hidden;
  padding-bottom: 30rpx;
}
/* search */
.label_input{
  width: 94%;
  margin: 10rpx 3% 34rpx;
  background: #fff;
  border-radius:37rpx;
  height: 74rpx;
  line-height: 70rpx;
  font-size: 14px;
}
.label_input input{
  height: 74rpx;
  line-height: 70rpx;
  float: left;
  margin-left: 14rpx;
  border: none;
}
.label_input .search_image{
  height: 30rpx;
  width: 30rpx;
  float: left;
  margin-left: 30rpx;
  margin-top: 24rpx;
}
.placeholder{
  color:#939393;
}
/* nav */
.label_nav{
  height: 210rpx;
  background: #fff;
  box-shadow:0px 3px 13px 0px rgba(153, 153, 153, 0.35);
  border-radius:10rpx;
  width: 92%;
  margin-left: 4%;
}
.nav_list{
  width: 25%;
  float: left;
  text-align: center;
  margin-top: 44rpx;
}
.nav_list .nav_img{
  width: 80rpx;
  height: 80rpx;
}
.nav_list .nav_text{
  color: #222222;
  font-size: 12px;
}
